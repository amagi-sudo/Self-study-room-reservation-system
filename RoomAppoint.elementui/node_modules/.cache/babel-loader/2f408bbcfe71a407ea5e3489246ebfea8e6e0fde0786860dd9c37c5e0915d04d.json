{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport store from '@/store';\nimport { mapGetters } from 'vuex';\nexport default {\n  data() {\n    return {\n      active: 'home'\n    };\n  },\n  mounted() {\n    console.log(this.UserInfo);\n  },\n  computed: {\n    ...mapGetters([\"UserInfo\", \"Token\"])\n  },\n  methods: {\n    ToLogin() {\n      this.$router.push({\n        path: '/Login'\n      });\n    },\n    ToRegister() {\n      this.$router.push({\n        path: '/Register'\n      });\n    },\n    async LoginOut() {\n      await this.$store.dispatch('Logout');\n      this.$router.push(`/Login`);\n    },\n    async ToPath(url) {\n      this.$router.push({\n        path: url\n      });\n    },\n    async ToUserInfo() {\n      this.$router.push({\n        path: \"/Front/UserPerson\"\n      });\n    },\n    async ToEditPassword() {\n      this.$router.push({\n        path: \"/Front/PasswordEdit\"\n      });\n    },\n    goBack() {\n      this.$router.go(-1);\n    }\n  }\n};","map":{"version":3,"names":["store","mapGetters","data","active","mounted","console","log","UserInfo","computed","methods","ToLogin","$router","push","path","ToRegister","LoginOut","$store","dispatch","ToPath","url","ToUserInfo","ToEditPassword","goBack","go"],"sources":["src/views/Front/Layout/index.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-container>\r\n      <el-header style=\"background-color: #333;\">\r\n        <el-menu :default-active=\"active\" class=\"main-container\" mode=\"horizontal\" text-color=\"purpleblue\"\r\n          active-text-color=\"white\">\r\n          <el-menu-item>\r\n            <img style=\"height: 60px;object-fit: cover;\" :src=\"require('@/assets/white.jpg')\">\r\n          </el-menu-item>\r\n          <el-menu-item index=\"/Front/Home\" @click=\"ToPath('/Front/Home')\" class=\"menu-transition\">IKUN\r\n            自习室预约</el-menu-item>\r\n\r\n          <el-menu-item style=\"float: right;\" v-if=\"!Token\" @click=\"ToRegister()\"\r\n            class=\"menu-transition\">注册</el-menu-item>\r\n\r\n          <el-menu-item style=\"float: right;\" v-if=\"!Token\" @click=\"ToLogin()\" class=\"menu-transition\">登录</el-menu-item>\r\n\r\n          <el-submenu style=\"float: right;font-weight: bold;\" v-if=\"Token\" index=\"userMenu\" class=\"menu-transition\">\r\n            <template slot=\"title\">{{ UserInfo.UserName }}</template>\r\n            <el-menu-item index=\"user个人信息\" @click=\"ToUserInfo()\">个人信息</el-menu-item>\r\n            <el-menu-item index=\"user修改密码\" @click=\"ToEditPassword()\">修改密码</el-menu-item>\r\n            <el-menu-item index=\"user退出\" @click=\"LoginOut()\">退出</el-menu-item>\r\n          </el-submenu>\r\n\r\n          <el-menu-item style=\"float: right;\" index=\"/Front/AppointRecordList\"\r\n            @click=\"ToPath('/Front/AppointRecordList')\" v-if=\"Token\" class=\"menu-transition\">我的预约记录</el-menu-item>\r\n\r\n          <el-menu-item style=\"float: right;\" index=\"/Front/IntegralList\" @click=\"ToPath('/Front/IntegralList')\"\r\n            v-if=\"Token\" class=\"menu-transition\">我的积分</el-menu-item>\r\n        </el-menu>\r\n      </el-header>\r\n\r\n      <el-main class=\"main-container main-box\">\r\n        <router-view></router-view>\r\n      </el-main>\r\n\r\n      <el-footer style=\"background-color: #1A2D6F; height: 80px;\">\r\n        <div class=\"footer-content\">\r\n          系统归属于IKUN团队，版权所有 ©2024-2025 IKUN团队\r\n        </div>\r\n      </el-footer>\r\n    </el-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport store from '@/store';\r\nimport { mapGetters } from 'vuex';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      active: 'home',\r\n    }\r\n  },\r\n  mounted() {\r\n    console.log(this.UserInfo);\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"UserInfo\", \"Token\"])\r\n  },\r\n  methods: {\r\n    ToLogin() {\r\n      this.$router.push({ path: '/Login' })\r\n    },\r\n    ToRegister() {\r\n      this.$router.push({ path: '/Register' })\r\n    },\r\n    async LoginOut() {\r\n      await this.$store.dispatch('Logout');\r\n      this.$router.push(`/Login`);\r\n    },\r\n    async ToPath(url) {\r\n      this.$router.push({ path: url });\r\n    },\r\n    async ToUserInfo() {\r\n      this.$router.push({ path: \"/Front/UserPerson\" })\r\n    },\r\n    async ToEditPassword() {\r\n      this.$router.push({ path: \"/Front/PasswordEdit\" })\r\n    },\r\n    goBack() {\r\n      this.$router.go(-1);\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 主容器样式 */\r\n.main-container {\r\n  width: 1300px;\r\n  margin: 0 auto;\r\n  font-weight: bold;\r\n}\r\n\r\n/* 菜单过渡效果 */\r\n.menu-transition {\r\n  transition: all 0.3s ease-in-out !important;\r\n  transform-origin: center;\r\n}\r\n\r\n/* 鼠标悬停效果 */\r\n.el-menu-item:not(.is-active):hover,\r\n.el-submenu:not(.is-active):hover .el-submenu__title {\r\n  opacity: 0.8;\r\n  transform: translateY(-2px);\r\n  background: rgba(255, 255, 255, 0.1) !important;\r\n}\r\n\r\n/* 激活状态动画 */\r\n.el-menu-item.is-active {\r\n  background-color: #409EFF !important;\r\n  color: #fff !important;\r\n  text-shadow: 0 0 5px #ff00ff, 0 0 10px #ff00ff;\r\n  animation: active-glow 0.5s ease-out;\r\n}\r\n\r\n@keyframes active-glow {\r\n  0% {\r\n    opacity: 0.8;\r\n    transform: scale(0.98);\r\n  }\r\n\r\n  50% {\r\n    opacity: 1;\r\n    transform: scale(1.02);\r\n  }\r\n\r\n  100% {\r\n    opacity: 1;\r\n    transform: scale(1);\r\n  }\r\n}\r\n\r\n/* 底部样式 */\r\n.el-footer {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  background-color: #1A2D6F !important;\r\n}\r\n\r\n.footer-content {\r\n  color: aliceblue;\r\n  font-size: 16px;\r\n  font-weight: bold;\r\n  text-align: center;\r\n  padding: 20px;\r\n  opacity: 0.9;\r\n  transition: opacity 0.3s ease;\r\n}\r\n\r\n.footer-content:hover {\r\n  opacity: 1;\r\n}\r\n\r\n/* 主内容区样式 */\r\n.main-box {\r\n  min-height: calc(100vh - 160px);\r\n  padding: 20px 0;\r\n}\r\n\r\n.el-main {\r\n  padding: 0 !important;\r\n  background: #f5f7fa;\r\n}\r\n\r\n/* 下拉菜单过渡 */\r\n.el-submenu .el-menu {\r\n  transition:\r\n    opacity 0.3s ease,\r\n    transform 0.3s ease;\r\n}\r\n\r\n.el-submenu .el-menu-item {\r\n  transition: background-color 0.2s ease;\r\n}\r\n</style>"],"mappings":";AA8CA,OAAAA,KAAA;AACA,SAAAC,UAAA;AAEA;EACAC,KAAA;IACA;MACAC,MAAA;IACA;EACA;EACAC,QAAA;IACAC,OAAA,CAAAC,GAAA,MAAAC,QAAA;EACA;EACAC,QAAA;IACA,GAAAP,UAAA;EACA;EACAQ,OAAA;IACAC,QAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QAAAC,IAAA;MAAA;IACA;IACAC,WAAA;MACA,KAAAH,OAAA,CAAAC,IAAA;QAAAC,IAAA;MAAA;IACA;IACA,MAAAE,SAAA;MACA,WAAAC,MAAA,CAAAC,QAAA;MACA,KAAAN,OAAA,CAAAC,IAAA;IACA;IACA,MAAAM,OAAAC,GAAA;MACA,KAAAR,OAAA,CAAAC,IAAA;QAAAC,IAAA,EAAAM;MAAA;IACA;IACA,MAAAC,WAAA;MACA,KAAAT,OAAA,CAAAC,IAAA;QAAAC,IAAA;MAAA;IACA;IACA,MAAAQ,eAAA;MACA,KAAAV,OAAA,CAAAC,IAAA;QAAAC,IAAA;MAAA;IACA;IACAS,OAAA;MACA,KAAAX,OAAA,CAAAY,EAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}