{"ast":null,"code":"// 保持原有JS逻辑不变（与上一版本相同）\nimport ValidCode from '@/components/Code/canvas.vue';\nimport store from '@/store';\nexport default {\n  components: {\n    ValidCode\n  },\n  data() {/* ... */},\n  created() {/* ... */},\n  mounted() {/* ... */},\n  beforeDestroy() {/* ... */},\n  methods: {/* 所有方法保持不变 */}\n};","map":{"version":3,"names":["ValidCode","store","components","data","created","mounted","beforeDestroy","methods"],"sources":["src/views/Login.vue"],"sourcesContent":["<template>\n  <div>\n    <canvas id=\"starrySky\"></canvas>\n    <div style=\"height: 100vh; display: flex; align-items: center; justify-content: center;\">\n      <div\n        style=\"background-color: white; padding: 30px 40px; border-radius: 15px; box-shadow: 0 8px 24px rgba(0, 0, 0, 0.1);\">\n        <div style=\"width: 400px;\">\n          <!-- 调整标题字体大小 -->\n          <h2 class=\"welcome-title\" style=\"margin-bottom: 25px; font-size: 24px;\">欢迎您~来到诚凛自习室预约系统</h2>\n          <el-form ref=\"loginForm\" :model=\"formData\" label-width=\"70px\" label-position=\"left\" :rules=\"rules\">\n            <!-- 账号输入 -->\n            <el-form-item label=\"账号\" prop=\"UserName\" style=\"margin-bottom: 18px;\">\n              <el-input type=\"text\" v-model.trim=\"formData.UserName\" placeholder=\"请输入账号\"\n                style=\"height: 40px; padding: 0 12px; font-size: 14px;\"></el-input>\n            </el-form-item>\n            <!-- 密码输入 -->\n            <el-form-item label=\"密码\" prop=\"Password\" style=\"margin-bottom: 18px;\">\n              <el-input type=\"password\" v-model.trim=\"formData.Password\" placeholder=\"请输入密码\"\n                style=\"height: 40px; padding: 0 12px; font-size: 14px;\"></el-input>\n            </el-form-item>\n            <!-- 角色选择（调整为同一行） -->\n            <el-form-item label=\"角色\" prop=\"RoleType\" style=\"margin-bottom: 18px;\">\n              <el-radio-group v-model=\"formData.RoleType\"\n                style=\"display: flex; gap: 15px; align-items: center; flex-wrap: nowrap;\">\n                <el-radio v-for=\"(item, index) in roleOptions\" :key=\"item.Code\" :label=\"item.Code\"\n                  style=\"font-size: 14px; white-space: nowrap;\">{{ item.Label }}</el-radio>\n              </el-radio-group>\n            </el-form-item>\n            <!-- 验证码区域（输入框与验证码同一行） -->\n            <el-form-item label=\"验证码\" prop=\"Code\"\n              style=\"display: flex; align-items: center; gap: 12px; margin-bottom: 22px;\">\n              <el-input type=\"text\" v-model.trim=\"formData.Code\" placeholder=\"请输入验证码\"\n                style=\"flex: 1; height: 40px; padding: 0 12px; font-size: 14px; min-width: 0;\"></el-input>\n              <div style=\"width: 100px; height: 40px;\"> <!-- 缩小验证码组件尺寸 -->\n                <ValidCode ref=\"ValidCode\"></ValidCode>\n              </div>\n            </el-form-item>\n            <!-- 按钮区域（缩小并居中） -->\n            <el-form-item style=\"display: flex; flex-direction: column; align-items: center; gap: 12px;\">\n              <el-button type=\"primary\" @click=\"LoginBtn\"\n                style=\"width: 160px; height: 42px; font-size: 15px; letter-spacing: 1px;\">登 录</el-button>\n              <el-button type=\"danger\" @click=\"ToHome\"\n                style=\"width: 160px; height: 42px; font-size: 15px; letter-spacing: 1px;\">前 台</el-button>\n            </el-form-item>\n          </el-form>\n          <!-- 底部注册/忘记密码 -->\n          <div\n            style=\"margin-top: 25px; display: flex; justify-content: space-between; align-items: center; gap: 10px; font-size: 13px;\">\n            <div style=\"display: flex; align-items: center; gap: 8px;\">\n              <span>还没有账号？</span>\n              <RouterLink :to=\"{ path: '/Register' }\" style=\"color: #F98C54; cursor: pointer; font-weight: 500;\">立即注册\n              </RouterLink>\n            </div>\n            <div style=\"color: #E91E63; cursor: pointer; font-weight: 500;\" @click=\"handleForgetPassword\">忘记密码？</div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n// 保持原有JS逻辑不变（与上一版本相同）\nimport ValidCode from '@/components/Code/canvas.vue'\nimport store from '@/store';\n\nexport default {\n  components: { ValidCode },\n  data() { /* ... */ },\n  created() { /* ... */ },\n  mounted() { /* ... */ },\n  beforeDestroy() { /* ... */ },\n  methods: { /* 所有方法保持不变 */ }\n}\n</script>\n\n<style scoped>\n.welcome-title {\n  /* 调整标题字体大小 */\n  font-size: 24px;\n  font-weight: 600;\n  background: linear-gradient(90deg, #4f6d7a 20%, #6a8b9a 50%, #8b9bb7 80%);\n  -webkit-background-clip: text;\n  color: transparent;\n  text-align: center;\n  letter-spacing: 1px;\n}\n\n#starrySky {\n  /* 保持原有背景样式 */\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  z-index: -1;\n  background: linear-gradient(180deg, #000033 0%, #000022 100%);\n}\n\n/* 调整输入框聚焦样式 */\n.el-input__inner:focus {\n  border-color: #4f6d7a !important;\n  box-shadow: 0 0 0 2px rgba(79, 109, 122, 0.1);\n}\n\n/* 保持按钮颜色样式 */\n.el-button--primary {\n  background-color: #4f6d7a;\n  border-color: #4f6d7a;\n}\n\n.el-button--primary:hover {\n  background-color: #6a8b9a;\n  border-color: #6a8b9a;\n}\n\n.el-button--danger {\n  background-color: #E91E63;\n  border-color: #E91E63;\n}\n\n.el-button--danger:hover {\n  background-color: #FF4081;\n  border-color: #FF4081;\n}\n</style>\n"],"mappings":"AA8DA;AACA,OAAAA,SAAA;AACA,OAAAC,KAAA;AAEA;EACAC,UAAA;IAAAF;EAAA;EACAG,KAAA;EACAC,QAAA;EACAC,QAAA;EACAC,cAAA;EACAC,OAAA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}