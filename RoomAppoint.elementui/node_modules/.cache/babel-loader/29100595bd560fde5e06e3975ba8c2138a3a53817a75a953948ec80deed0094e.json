       required: true,\n          message: '该项为必填项',\n          trigger: 'blur'\n        }]\n      },\n      formData: {},\n      //保存或者修改定义的数据对象\n      editorShow: false\n    };\n  },\n  created() {},\n  methods: {\n    //到场\n    async ArrivalClock(id) {\n      let {\n        Data\n      } = await this.$PostSigleUpdate(`/AppointRecord/Get`, `/AppointRecord/ArrivalClock`, id, '确定要打卡了吗？', {});\n      this.$refs.PaginationTableId.Reload(this.searchForm);\n    },\n    //取消预约\n    async CancelAppoint(id) {\n      let {\n        Data\n      } = await this.$PostSigleUpdate(`/AppointRecord/Get`, `/AppointRecord/CancelAppoint`, id, '确定要取消预约吗？', {});\n      this.$refs.PaginationTableId.Reload(this.searchForm);\n    },\n    // 提前结束\n    async EarlyEnd(id) {\n      let {\n        Data\n      } = await this.$PostSigleUpdate(`/AppointRecord/Get`, `/AppointRecord/EarlyEnd`, id, '确定要提前结束预约吗？', {});\n      this.$refs.PaginationTableId.Reload(this.searchForm);\n    },\n    //修改属性\n    async UpdateEntityAsync(Id, title, data) {\n      let {\n        Data\n      } = await this.$PostSigleUpdate(`/AppointRecord/Get`, `/AppointRecord/CreateOrEdit`, Id, title, data);\n      this.$refs.PaginationTableId.Reload(this.searchForm);\n    },\n    /**\r\n    * 点击评论\r\n    */\n    async ShowEditModal(Id) {\n      try {\n        let response = await this.$Post(`/AppointRecord/Get`, {\n          Id: Id\n        });\n        if (response.Success) {\n          this.formData = response.Data;\n          this.editorShow = true;\n        } else {\n          this.$message.error('获取评论信息失败');\n        }\n      } catch (error) {\n        console.error('获取评论信息时发生错误:', error);\n        this.$message.error('获取评论信息时发生错误');\n      }\n    },\n    /**\r\n    * 评论保存\r\n    */\n    async CreateOrEditForm() {\n      this.$refs.editModalForm.validate(async valid => {\n        if (valid) {\n          try {\n            var response = await this.$Post(`/AppointRecord/SComment`, this.formData);\n            if (response.Success) {\n              this.editorShow = false;\n              this.$refs.PaginationTableId.Reload(this.searchForm);\n              this.$message.success('评论保存成功');\n            } else {\n              this.$message.error('评论保存失败');\n            }\n          } catch (error) {\n            console.error('评论保存时发生错误:', error);\n            this.$message.error('评论保存时发生错误');\n          }\n        }\n      });\n    },\n    /**\r\n     * 单个删除的时候会触发\r\n     */\n    async ShowDeleteModal(Id) {\n      await this.$PostDelete(`/AppointRecord/Delete`, {\n        Id: Id\n      });\n      this.$refs.PaginationTableId.Reload(this.searchForm);\n    },\n    /**\r\n     * 批量删除的时候会触发\r\n     */\n    async ShowBatchDeleteModal() {\n      var ids = this.$refs.PaginationTableId.GetSelectionRow().map(x => x.Id);\n      if (ids.length == 0) {\n        this.$message.error(\"你选择需要删除的记录\");\n        return;\n      }\n      await this.$PostDelete(`/AppointRecord/BatchDelete`, {\n        Ids: ids\n      });\n      this.$refs.PaginationTableId.Reload(this.searchForm);\n    },\n    /**\r\n     * 点击表格搜索按钮会触发\r\n     */\n    async SearchClick() {\n      this.$refs.PaginationTableId.Reload(this.searchForm);\n    },\n    /**\r\n     * 点击清空表单会触发\r\n     */\n    async ResetClick() {\n      this.searchForm = {};\n      this.$refs.PaginationTableId.Reload(this.searchForm);\n    }\n  }\n};","map":{"version":3,"names":["store","mapGetters","name","computed","data","where","UserId","getters","searchForm","dataColum","key","hidden","title","width","type","ColumnType","SHORTTEXT","DATE","DATETIME","USERDEFINED","editModalFormRules","required","message","trigger","formData","editorShow","created","methods","ArrivalClock","id","Data","$PostSigleUpdate","$refs","PaginationTableId","Reload","CancelAppoint","EarlyEnd","UpdateEntityAsync","Id","ShowEditModal","response","$Post","Success","$message","error","console","CreateOrEditForm","editModalForm","validate","valid","success","ShowDeleteModal","$PostDelete","ShowBatchDeleteModal","ids","GetSelectionRow","map","x","length","Ids","SearchClick","ResetClick"],"sources":["src/views/Front/AppointRecordList.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-container margin-top-lg\">\r\n    <el-card class=\"box-card\">\r\n      <div slot=\"header\" class=\"clearfix\">\r\n        <el-button type=\"primary\" size=\"mini\" icon=\"el-icon-search\" @click=\"SearchClick\">搜 索</el-button>\r\n        <el-button type=\"warning\" size=\"mini\" @click=\"ResetClick\" icon=\"el-icon-s-promotion\">重 置</el-button>\r\n      </div>\r\n      <div class=\"tb-body\">\r\n        <el-form ref=\"searchFormRef\" :model=\"searchForm\" :inline=\"true\" label-width=\"120px\" :style=\"`flex:1;`\">\r\n          <el-form-item label=\"流水编号\" prop=\"NoLike\">\r\n            <el-input v-model.trim=\"searchForm.NoLike\" placeholder=\"请输入流水编号\" :clearable=\"true\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"手机号\" prop=\"PhoneLike\">\r\n            <el-input v-model.trim=\"searchForm.PhoneLike\" placeholder=\"请输入手机号\" :clearable=\"true\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"姓名\" prop=\"NameLike\">\r\n            <el-input v-model.trim=\"searchForm.NameLike\" placeholder=\"请输入姓名\" :clearable=\"true\"></el-input>\r\n          </el-form-item>\r\n\r\n          <el-form-item label=\"自习室\">\r\n            <SigleSelect url=\"/Room/List\" columnName=\"Name\" :clearable=\"true\" columnValue=\"Id\"\r\n              v-model=\"searchForm.RoomId\">\r\n            </SigleSelect>\r\n          </el-form-item>\r\n          <el-form-item label=\"座位\">\r\n            <SigleSelect url=\"/Seat/List\" columnName=\"No\" :clearable=\"true\" columnValue=\"Id\"\r\n              v-model=\"searchForm.SeatId\">\r\n            </SigleSelect>\r\n          </el-form-item>\r\n\r\n          <el-form-item label=\"预约状态\">\r\n            <SigleSelect url=\"/Select/AppointStatusEnum\" columnName=\"Name\" :clearable=\"true\" columnValue=\"Code\"\r\n              v-model=\"searchForm.AppointStatus\">\r\n            </SigleSelect>\r\n          </el-form-item>\r\n          <el-form-item label=\"起始时间\">\r\n            <el-date-picker v-model=\"searchForm.BeginTimeRange\" type=\"datetimerange\" start-placeholder=\"开始起始时间\"\r\n              end-placeholder=\"结束起始时间\" :default-time=\"['00:00:00']\" value-format=\"yyyy-MM-dd HH:mm:ss\">\r\n            </el-date-picker>\r\n          </el-form-item>\r\n          <el-form-item label=\"截至时间\">\r\n            <el-date-picker v-model=\"searchForm.EndTimeRange\" type=\"datetimerange\" start-placeholder=\"开始截至时间\"\r\n              end-placeholder=\"结束截至时间\" :default-time=\"['00:00:00']\" value-format=\"yyyy-MM-dd HH:mm:ss\">\r\n            </el-date-picker>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n\r\n    </el-card>\r\n    <el-dialog title=\"评论评分\" :visible.sync=\"editorShow\" width=\"50%\" :lock-scroll=\"true\" height=\"800px\">\r\n      <el-form v-if=\"editorShow == true\" ref=\"editModalForm\" :rules=\"editModalFormRules\" :model=\"formData\"\r\n        label-width=\"140px\" size=\"mini\">\r\n        <el-row :gutter=\"10\" class=\"EditFromBody\">\r\n          <el-col :span=\"24\">\r\n            <el-form-item label=\"评论\" prop=\"Comment\">\r\n              <el-input type=\"textarea\" :rows=\"10\" v-model=\"formData.Comment\" placeholder=\"请输入评论\"\r\n                :clearable=\"true\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"24\">\r\n            <el-form-item label=\"评分\" prop=\"CommentScore\">\r\n              <el-rate v-model=\"formData.CommentScore\"></el-rate>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n\r\n\r\n        <el-row type=\"flex\" justify=\"end\" align=\"bottom\">\r\n          <el-form-item>\r\n            <el-button type=\"primary\" plain @click=\"CreateOrEditForm()\">确 定</el-button>\r\n            <el-button @click=\"editorShow = false\">取 消</el-button>\r\n          </el-form-item>\r\n        </el-row>\r\n      </el-form>\r\n\r\n    </el-dialog>\r\n    <PaginationTable ref=\"PaginationTableId\" url=\"/AppointRecord/List\" :column=\"dataColum\" :where=\"where\">\r\n      <template v-slot:Operate=\"scope\">\r\n        <el-button type=\"primary\" size=\"mini\" v-if=\"scope.row.AppointStatus == 6\" @click=\"ShowEditModal(scope.row.Id)\">评\r\n          论</el-button>\r\n        <el-button type=\"primary\" size=\"mini\" v-if=\"scope.row.AppointStatus == 1\"\r\n          @click=\"ArrivalClock(scope.row.Id)\">到场打卡</el-button>\r\n        <el-button type=\"warning\" size=\"mini\" v-if=\"scope.row.AppointStatus == 2\"\r\n          @click=\"EarlyEnd(scope.row.Id)\">提前结束</el-button>\r\n        <el-button type=\"danger\" size=\"mini\" v-if=\"scope.row.AppointStatus == 1\"\r\n          @click=\"CancelAppoint(scope.row.Id)\">取消预约</el-button>\r\n      </template>\r\n    </PaginationTable>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport store from '@/store';\r\nimport { mapGetters } from 'vuex'\r\nexport default {\r\n  name: \"AppointRecordList\",\r\n  computed: {\r\n    ...mapGetters([\r\n      'Token', 'UserInfo', 'RoleType', 'HasUserInfo', 'ColumnType', \"UserId\"\r\n    ])\r\n  },\r\n  data() {\r\n\r\n    return {\r\n      where: {\r\n        UserId: store.getters.UserId,\r\n      },\r\n      searchForm: {},//搜索定义的数据对象\r\n      dataColum: [\r\n        {\r\n          key: \"Id\",\r\n          hidden: true,\r\n\r\n        },\r\n        {\r\n          key: \"RoomId\",\r\n          hidden: true,\r\n        },\r\n        {\r\n          key: \"No\",\r\n          title: \"流水编号\",\r\n          width: \"220px\",\r\n\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n        },\r\n        {\r\n          key: \"RoomDto.Name\",\r\n          title: \"自习室名称\",\r\n          width: \"160px\",\r\n\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n        },\r\n        {\r\n          key: \"SeatId\",\r\n          hidden: true,\r\n        },\r\n        {\r\n          key: \"SeatDto.No\",\r\n          title: \"座位编号\",\r\n          width: \"160px\",\r\n\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n        },\r\n        {\r\n          key: \"UserId\",\r\n          hidden: true,\r\n        },\r\n        {\r\n          key: \"AppointDate\",\r\n          title: \"预约日期\",\r\n          width: \"160px\",\r\n\r\n          type: store.getters.ColumnType.DATE,\r\n        },\r\n        {\r\n          key: \"AppointDateTypeFormat\",\r\n          title: \"预约时间段\",\r\n          width: \"160px\",\r\n\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n        },\r\n        {\r\n          key: \"UserDto.Name\",\r\n          title: \"用户名称\",\r\n          width: \"160px\",\r\n\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n        },\r\n        {\r\n          key: \"Phone\",\r\n          title: \"手机号\",\r\n          width: \"160px\",\r\n\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n        },\r\n        {\r\n          key: \"Name\",\r\n          title: \"姓名\",\r\n          width: \"160px\",\r\n\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n        },\r\n\r\n        {\r\n          key: \"BeginTime\",\r\n          title: \"签到时间\",\r\n          width: \"240px\",\r\n          type: store.getters.ColumnType.DATETIME,\r\n        },\r\n        {\r\n          key: \"EndTime\",\r\n          title: \"离开时间\",\r\n          width: \"240px\",\r\n          type: store.getters.ColumnType.DATETIME,\r\n        },\r\n        {\r\n          key: \"CommentScore\",\r\n          title: \"评分\",\r\n          width: \"160px\",\r\n\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n        },\r\n        {\r\n          key: \"Comment\",\r\n          title: \"评论\",\r\n          width: \"160px\",\r\n\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n        },\r\n        {\r\n          key: \"AppointStatus\",\r\n          hidden: true,