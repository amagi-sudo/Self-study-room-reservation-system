{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport store from '@/store';\nimport { mapGetters } from 'vuex';\nimport * as THREE from 'three';\nexport default {\n  data() {\n    return {\n      active: 'home'\n    };\n  },\n  mounted() {\n    this.initCanvas();\n    console.log(this.UserInfo);\n  },\n  computed: {\n    ...mapGetters(['UserInfo', 'Token'])\n  },\n  methods: {\n    ToLogin() {\n      this.$router.push({\n        path: '/Login'\n      });\n    },\n    ToRegister() {\n      this.$router.push({\n        path: '/Register'\n      });\n    },\n    async LoginOut() {\n      await this.$store.dispatch('Logout');\n      this.$router.push('/Login');\n    },\n    async ToPath(url) {\n      this.$router.push({\n        path: url\n      });\n    },\n    async ToUserInfo() {\n      this.$router.push({\n        path: '/Front/UserPerson'\n      });\n    },\n    async ToEditPassword() {\n      this.$router.push({\n        path: '/Front/PasswordEdit'\n      });\n    },\n    goBack() {\n      this.$router.go(-1);\n    },\n    initCanvas() {\n      const canvas = document.getElementById('backgroundCanvas');\n      const renderer = new THREE.WebGLRenderer({\n        canvas\n      });\n      const scene = new THREE.Scene();\n      const camera = new THREE.PerspectiveCamera(75, window.innerWidth / window.innerHeight, 0.1, 1000);\n      camera.position.z = 5;\n      class Shape {\n        constructor(x, y, z, speedX, speedY, speedZ) {\n          this.x = x;\n          this.y = y;\n          this.z = z;\n          this.speedX = speedX;\n          this.speedY = speedY;\n          this.speedZ = speedZ;\n        }\n        update() {\n          this.x += this.speedX;\n          this.y += this.speedY;\n          this.z += this.speedZ;\n          const boundary = 5;\n          if (this.x < -boundary || this.x > boundary) {\n            this.speedX = -this.speedX;\n          }\n          if (this.y < -boundary || this.y > boundary) {\n            this.speedY = -this.speedY;\n          }\n          if (this.z < -boundary || this.z > boundary) {\n            this.speedZ = -this.speedZ;\n          }\n        }\n        draw() {\n          const material = new THREE.MeshBasicMaterial({\n            color: 0x00001e,\n            opacity: 0.5,\n            transparent: true\n          });\n\n          // 绘制圆形\n          const circleGeometry = new THREE.CircleGeometry(0.5, 32);\n          const circleMesh = new THREE.Mesh(circleGeometry, material);\n          circleMesh.position.set(this.x, this.y, this.z);\n          scene.add(circleMesh);\n\n          // 绘制正方体\n          const cubeGeometry = new THREE.BoxGeometry(0.5, 0.5, 0.5);\n          const cubeMesh = new THREE.Mesh(cubeGeometry, material);\n          cubeMesh.position.set(this.x, this.y, this.z);\n          scene.add(cubeMesh);\n\n          // 绘制三棱锥\n          const tetrahedronGeometry = new THREE.TetrahedronGeometry(0.5);\n          const tetrahedronMesh = new THREE.Mesh(tetrahedronGeometry, material);\n          tetrahedronMesh.position.set(this.x, this.y, this.z);\n          scene.add(tetrahedronMesh);\n        }\n      }\n      const shapes = [];\n      const numShapes = 10;\n      for (let i = 0; i < numShapes; i++) {\n        const x = Math.random() * 10 - 5;\n        const y = Math.random() * 10 - 5;\n        const z = Math.random() * 10 - 5;\n        const speedX = Math.random() * 0.1 - 0.05;\n        const speedY = Math.random() * 0.1 - 0.05;\n        const speedZ = Math.random() * 0.1 - 0.05;\n        shapes.push(new Shape(x, y, z, speedX, speedY, speedZ));\n      }\n      const animate = () => {\n        requestAnimationFrame(animate);\n        renderer.render(scene, camera);\n        while (scene.children.length > 0) {\n          scene.remove(scene.children[0]);\n        }\n        shapes.forEach(shape => {\n          shape.update();\n          shape.draw();\n        });\n      };\n      animate();\n    }\n  }\n};","map":{"version":3,"names":["store","mapGett